<%- include('partials/header'); -%> 

    <h1>Compose</h1>
    
    <form action="/user/<%= user %>/compose" class="" method="post">
        <div class="form-group">
            <label for="postTitle">Title</label>
            <input type="text" class="form-control" id="postTitle" name="postTitle" placeholder="Enter">
        </div>
        <div class="form-group">
            <label for="postContentid">Post</label>
            <textarea class="form-control" name="postBody" id="postContentid" rows="4"></textarea>
        </div>
        <button class="btn btn-primary" type="submit">publish</button>
    </form>

<%- include('partials/footer'); -%> 